  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.14.0",
    "@types/tough-cookie": "^4.0.5",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "axios-cookiejar-support": "^5.0.5",
    "cheerio": "^1.0.0",
    "tough-cookie": "^5.1.2"
