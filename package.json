{
  "name": "mobidziennik-sdk",
  "version": "1.2.0",
  "description": "TypeScript web scraping SDK for Polish e-gradebook MobiDziennik",
  "license": "GPL",
  "author": "Norbiros",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "echo Test!",
    "build": "tsc",
    "format": "prettier --write src/",
    "lint": "eslint src/",
