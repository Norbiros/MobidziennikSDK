    "version": "pnpm format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Norbiros/MobidziennikSDK.git"
  },
  "keywords": [
    "mobidziennik",
    "sdk",
    "typescript",
    "e-gradebook",
    "webscraping"
